 <table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Phone</th>
      <th>Mobile</th>
      <th>Street number</th>
      <th>Addressline 1</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Postcode</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.surname %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.mobile %></td>
        <td><%= profile.street_number %></td>
        <td><%= profile.addressline_1 %></td>
        <td><%= profile.suburb %></td>
        <td><%= profile.state %></td>
        <td><%= profile.postcode %></td>
        <td><%= profile.user %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

